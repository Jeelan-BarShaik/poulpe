<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.jtalks.poulpe.model.entity">
  <class name="Rank" table="Rank">
    <id column="RANK_ID" name="id" unsaved-value="0">
      <generator class="native" />
    </id>
    <property column="UUID" name="uuid" not-null="true" unique="true" />
    <property column="RANK_NAME" name="rankName" not-null="true" unique="true"/>
    <property column="AUTO_ASSIGNED" name="autoAssigned" />
    <property column="POST_COUNT" name="postCount" />
  </class>
</hibernate-mapping>
